  },
  "dependencies": {
    "ethers": "^6.13.4",
    "@pancakeswap/sdk": "^5.7.0",
    "@bnb-chain/greenfield-js-sdk": "^2.0.0",
    "telegraf": "^4.16.3",
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2",
    "winston": "^3.14.2",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "ai": "^5.0.81",
    "zod": "^3.23.8",
    "long": "^5.2.3"
  },
