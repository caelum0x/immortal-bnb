{
  "name": "immortal-bnb",
  "version": "1.0.0",
  "description": "An autonomous AI trading agent for BNB Chain with immortal memory storage on BNB Greenfield",
  "main": "src/index.ts",
  "scripts": {
    "start": "bash start-all.sh",
    "start:all": "bash start-all.sh",
    "dev": "bun run --watch src/index.ts",
    "dev:frontend": "bun --cwd frontend dev",
    "dev:backend": "bun run --watch src/index.ts",
    "dev:full": "concurrently \"bun run dev:backend\" \"bun run dev:frontend\"",
    "bot": "bun index.ts",
    "setup": "bash scripts/setup.sh",
    "check": "bun start-bot.ts",
    "frontend": "cd frontend && bun dev",
    "backend": "bun src/index.ts",
    "install:all": "bun install && cd frontend && bun install",
    "build": "bun build src/index.ts --outdir dist --target node",
    "build:frontend": "bun --cwd frontend build",
    "build:all": "bun run build && bun run build:frontend",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "bun test-integration.ts",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:load": "jest --testPathPattern=tests/load",
    "test:trade": "bun test-trade.ts",
    "test:polymarket": "bun test-polymarket.ts",
    "polymarket:demo": "bun polymarket-live-demo.ts",
    "lint": "eslint src/**/*.ts",
    "healthcheck": "bun run scripts/healthcheck.ts",
    "monitor": "bun run scripts/monitor.ts",
    "monitor:watch": "bun run scripts/monitor.ts --watch",
    "backup": "bash scripts/backup.sh",
    "docker:build": "docker build -t immortal-bot .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f bot",
    "validate": "npx ts-node scripts/validate-deployment.ts",
    "perf": "bun run scripts/performance.ts --current",
    "perf:report": "bun run scripts/performance.ts --report",
    "perf:collect": "bun run scripts/performance.ts --collect",
    "contracts:build": "forge build",
    "contracts:test": "forge test",
    "contracts:deploy": "bash scripts/foundry-deploy.sh",
    "contracts:deploy-manual": "bun run scripts/deploy-contracts.ts",
    "cli": "bun cli.ts",
    "status": "bun cli.ts status",
    "balance": "bun cli.ts balance",
    "trades": "bun cli.ts trades",
    "stats": "bun cli.ts stats"
  },
  "keywords": [
    "ai",
    "trading",
    "bnb",
    "blockchain",
    "defi",
    "autonomous",
    "pancakeswap",
    "greenfield",
    "immortal-memory"
  ],
  "author": "arhansubasi0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caelum0x/immortal-bnb"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.63",
    "@bnb-chain/greenfield-js-sdk": "^2.0.0",
    "@ethersproject/bytes": "^5.8.0",
